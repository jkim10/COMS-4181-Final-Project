#!/usr/bin/expect -f

spawn ../client/getcert
match_max 100000
expect -exact "Username: "
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "12345678901234567890123456789012345678901034567890"
expect -exact "12345678901234567890123456789012345678901034567890"
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "1\r"
expect eof

set timeout -1
spawn ../client/getcert
match_max 100000
expect -exact "Username: "
send -- "addleness\r"
expect -exact "addleness\r
Password: "
send -- "\r"
expect eof

set timeout -1
spawn ../client/changepw
match_max 100000
expect -exact "Username: "
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "12345678901234567890123456789012345678901034567890"
expect -exact "12345678901234567890123456789012345678901034567890"
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "12345678901234567890123456789012345678901234567890"
expect -exact "12345678901234567890123456789012345678901234567890"
send -- "1\r"
expect eof

spawn ../client/changepw
match_max 100000
expect -exact "Username: "
send -- "addleness\r"
expect -exact "addleness\r
Password: "
send -- "\r"
expect eof